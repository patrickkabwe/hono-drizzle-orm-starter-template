{
  "name": "hono-drizzle-orm-starter-template",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "typecheck": "tsc",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "migrate": "tsx app/database/migrate.ts",
    "generate": "drizzle-kit generate:pg",
    "push": "drizzle-kit push:pg",
    "drop": "drizzle-kit drop",
    "studio": "drizzle-kit studio --port 5555"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.2",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.29.4",
    "drizzle-zod": "^0.5.1",
    "hono": "^4.0.9",
    "jose": "^5.2.2",
    "pino": "^8.19.0",
    "postgres": "^3.4.3",
    "socket.io": "^4.7.4",
    "vitest": "^1.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "bun": "^1.0.29",
    "tsx": "^3.12.2",
    "vite-tsconfig-paths": "^4.3.1"
  }
}
